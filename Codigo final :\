
const int SER = 3;
const int RCLK = 4;
const int SRCLK = 5;

const int SER2 = 10;
const int RCLK2 = 11;
const int SRCLK2 = 12;

void _RCLK (int RCLK);
void _SRCLK (int SRCLK);

void _RCLK2 (int RCLK2);
void _SRCLK2 (int SRCLK2);


int puertos1[3] = {SER,RCLK,SRCLK};
int puertos2[3] = {SER2,RCLK2,SRCLK2};



void setup(){
	//(paso 1) configuracion de puertos digitales como salida
   
  for(int i = 0; i<3 ; i++){
    pinMode(puertos1[i] , OUTPUT);
    digitalWrite(puertos1[i] , 0);}
  
   for(int i = 0; i<3 ; i++){
    pinMode(puertos2[i] , OUTPUT);
    digitalWrite(puertos2[i] , 0);}
  
  for (int i = 0; i<8 ; i++){
    	digitalWrite(SER , 1);
  		_SRCLK (SRCLK);
		_RCLK (RCLK);
  
  }
  
  	
  	//(paso 2) llevar dato al puerto serial del 74HC595 1/0
  
    //*(*(puntero_cuadro + FILAS) + COLUMNAS);
 int columna1[8] = {0,0,0,0,0,0,0,0};//estos arreglos representan
 int columna2[8] = {0,0,1,0,0,1,0,0};//los patrones deseados
 int columna3[8] = {0,0,0,0,0,0,0,0};
 int columna4[8] = {0,0,0,0,0,0,0,0};
