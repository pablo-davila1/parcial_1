}
void loop(){
}

void _RCLK (int RCLK){
    digitalWrite(RCLK , 0);
  	digitalWrite(RCLK , 1);
  	digitalWrite(RCLK , 0);} 

void _SRCLK (int SRCLK){
    digitalWrite(SRCLK , 0);
  	digitalWrite(SRCLK , 1);
  	digitalWrite(SRCLK , 0);}

void _RCLK2 (int RCLK2){
    digitalWrite(RCLK2 , 0);
  	digitalWrite(RCLK2 , 1);
  	digitalWrite(RCLK2 , 0);} 

void _SRCLK2 (int SRCLK2){
    digitalWrite(SRCLK2 , 0);
  	digitalWrite(SRCLK2 , 1);
  	digitalWrite(SRCLK2 , 0);}
