 int c=0;
  

//------------ un solo patron----------  
  while (c<=2){
  //-------columna 1-------------
    for(int i =1; i<=8 ; i++){
    if(columna1[i-1]!=0){
      	digitalWrite(SER , 1);     //Estos ciclos buscan en los 
  		_SRCLK (SRCLK);			   //arreglos correspondientes a
        for(int j =1; j<=8 ; j++){ // las filas n y columnas n
	
    	if(j==8){
      	 	digitalWrite(SER2 , 0);
  			_SRCLK2 (SRCLK2);
			_RCLK2 (RCLK2);}    
    	else{
     		digitalWrite(SER2 , 1);
  			_SRCLK2 (SRCLK2);}
  		}
    
    
    }
      

   else if(i==8){
      	digitalWrite(SER , 0);
  		_SRCLK (SRCLK);
		_RCLK (RCLK);}
      
   else{
       digitalWrite(SER , 0);
    	_SRCLK (SRCLK);}
   }
  

 delay(55) ;
  //-------columna 2------------------ 
for(int i =1; i<=8 ; i++){
